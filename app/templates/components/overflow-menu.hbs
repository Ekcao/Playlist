{{#paper-menu as |menu|}}
    {{#menu.trigger}}
        {{#paper-button iconButton=true}}
            {{paper-icon "more_vert"}}
        {{/paper-button}}
    {{/menu.trigger}}
    {{#menu.content as |content|}}
        {{#content.menu-item onClick=(action signOut)}}
            <span>Sign Out</span>
        {{/content.menu-item}}
    {{/menu.content}}
{{/paper-menu}}
