{{#if firebaseSession.isAuthenticated}}
    Logged in as {{firebaseSession.currentUser.displayName}}
    {{#paper-button onClick=(action "signOut")}}
        Sign out
    {{/paper-button}}
{{else}}
    {{#paper-button class="sign-in-button" onClick=(action "signIn" "google")}}
        Sign in with Google
    {{/paper-button}}
{{/if}}
